{
  "name": "serverless-standalone",
  "version": "0.0.15",
  "description": "Emulate AWS Î» and aws-sdk locally when developing your Serverless node.js project",
  "keywords": [
    "serverless",
    "aws-lambda"
  ],
  "license": "ISC",
  "author": "if1live",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/*",
    "dist/src/*",
    "!dist/test/*",
    "!dist/e2e/*"
  ],
  "scripts": {
    "build": "tsc",
    "check:watch": "tsc --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "fix": "prettier --write \"**/*.{ts,mjs,md}\"",
    "circularDepCheck": "madge --circular src"
  },
  "resolutions": {
    "aws-sdk": "2.1311.0"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "~3.188.0",
    "cron": "^2.4.4",
    "debug": "^4.3.4",
    "http-terminator": "^3.2.0",
    "mqtt": "^5.3.3",
    "remeda": "^1.30.0",
    "ws": "^8.15.0"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "~3.188.0",
    "@aws-sdk/client-lambda": "~3.188.0",
    "@types/aws-lambda": "^8.10.130",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.10.4",
    "@types/ws": "^8.5.10",
    "conditional-type-checks": "^1.0.6",
    "madge": "^6.1.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "packageManager": "pnpm@8.12.0"
}
